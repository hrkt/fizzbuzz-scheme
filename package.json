{
  "name": "fizzbuzz-scheme",
  "version": "0.1.6",
  "description": "A tiny scheme subset interpreter that can evaluate fizz-buzz code.",
  "main": "src/index.js",
  "scripts": {
    "clean": "rimraf isolate-* coverage",
    "fib30": "node src/fbs sample/fibonacci.scm sample/fib30.scm",
    "lint": "npx eslint src/ test/",
    "repl": "node src/repl",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest",
    "coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules npx jest --collectCoverage",
    "majestic": "cross-env NODE_OPTIONS=--experimental-vm-modules npx majestic",
    "show-opt-deopt-win": "node --trace-deopt --trace-opt .\\src\\fbs.js .\\sample\\fibonacci.scm .\\sample\\fib30.scm",
    "perf-get-prof-linux": "node --prof ./src/fbs.js ./sample/fibonacci.scm ./sample/fib30.scm",
    "perf-get-prof-win": "node --prof .\\src\\fbs.js .\\sample\\fibonacci.scm .\\sample\\fib30.scm",
    "perf-parse-prof-linux": "node --prof-process isolate-*",
    "perf-parse-prof-win": "powershell -c node --prof-process (Get-Item .\\isolate-*)",
    "perf-linux": "run-s perf-get-prof-linux perf-parse-prof-linux",
    "perf-win": "run-s perf-get-prof-win perf-parse-prof-win",
    "sloc": "npx sloc . -e node_modules"
  },
  "keywords": [
    "scheme",
    "interpreter"
  ],
  "author": "Hiroki Ito",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "cross-env": "^7.0.3",
    "loglevel": "^1.7.1",
    "prompt-sync": "^4.2.0"
  },
  "type": "module",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.31.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "jest": "^27.2.1",
    "jest-mock-process": "^1.4.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/**/fbs.js",
      "!src/**/repl.js"
    ],
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text"
    ]
  }
}
